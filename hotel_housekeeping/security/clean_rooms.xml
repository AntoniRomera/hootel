<odoo>

  <record model="ir.model.access" id="clean_rooms_access_user">
      <field name="name">clean_rooms access hotel user</field>
      <field name="model_id" ref="model_clean_rooms"/>
      <field name="group_id" ref="hotel.group_hotel_user"/>
      <field name="perm_read" eval="1"/>
      <field name="perm_create" eval="1"/>
      <field name="perm_write" eval="1"/>
      <field name="perm_unlink" eval="0"/>
  </record>

  <record model="ir.model.access" id="clean_rooms_access_manager">
      <field name="name">clean_rooms access hotel manager</field>
      <field name="model_id" ref="model_clean_rooms"/>
      <field name="group_id" ref="hotel.group_hotel_manager"/>
      <field name="perm_read" eval="1"/>
      <field name="perm_create" eval="1"/>
      <field name="perm_write" eval="1"/>
      <field name="perm_unlink" eval="1"/>
  </record>

  <record model="ir.model.access" id="clean_rooms_access_housekeeper">
      <field name="name">clean_rooms access housekeeper</field>
      <field name="model_id" ref="model_clean_rooms"/>
      <field name="group_id" ref="hotel_group_housekeeper"/>
      <field name="perm_read" eval="1"/>
      <field name="perm_create" eval="1"/>
      <field name="perm_write" eval="1"/>
      <field name="perm_unlink" eval="0"/>
  </record>

</odoo>
